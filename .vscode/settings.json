{
  "editor.formatOnSave": true,
  "cSpell.words": [
    "aipictors",
    "appbundle",
    "audioplayers",
    "autovalidate",
    "cupertino",
    "followee",
    "followees",
    "horiz",
    "MMMMEEE",
    "prompton",
    "promptonio",
    "riverpod",
    "subtags",
    "unfocus",
    "unfollow"
  ],
  "files.associations": {
    "*.xml": "html",
    "*.svg": "html"
  },
  "files.exclude": {
    "**/*.ast.gql.dart": true,
    "**/*.data.gql.dart": true,
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/*.gql.g.dart": true,
    "**/*.req.gql.dart": true,
    "**/*.var.gql.dart": true,
    "**/.idea": true,
    "**/__generated__": true,
    "*.iml": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".idea": true,
    ".metadata": true,
    ".yarn": true,
    "android/.gradle": true,
    "android/aipictors_android.iml": true,
    "android/gradle": true,
    "android/gradlew": true,
    "android/gradlew.bat": true,
    "android/local.properties": true,
    "build": true,
    "ios/.symlinks": true,
    "ios/Pods": true,
    "ios/Runner.xcodeproj": false,
    "ios/Runner.xcworkspace": true,
    "ios/Runner/GeneratedPluginRegistrant.h": true,
    "ios/Runner/GeneratedPluginRegistrant.m": true,
    "node_modules": true,
    "package-lock.json": true,
    "pubspec.lock": true
  },
  "cSpell.ignorePaths": [
    ".vscode",
    "android",
    "assets",
    "ios",
    "lib/schema.graphql",
    "linux",
    "web",
    "windows"
  ],
  "[json]": {
    "editor.codeActionsOnSave": {
      "quickfix.biome": true
    },
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "quickfix.biome": true,
      "source.organizeImports.biome": true
    },
    "editor.defaultFormatter": "biomejs.biome"
  }
}
