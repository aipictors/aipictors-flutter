{
  "editor.formatOnSave": true,
  "cSpell.words": ["aipictors", "followees", "MMMMEEE", "riverpod", "unfocus"],
  "files.associations": {
    "*.xml": "html",
    "*.svg": "html"
  },
  "files.exclude": {
    "**/*.ast.gql.dart": true,
    "**/*.data.gql.dart": true,
    "**/*.gql.g.dart": true,
    "**/*.req.gql.dart": true,
    "**/*.var.gql.dart": true,
    "**/__generated__": true,
    "*.iml": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".idea": true,
    ".metadata": true,
    "build": true,
    "pubspec.lock": true
  }
}
